<h2 class="header">New Provice 
	<a class="right waves-effect waves-light btn" href="/admin/provices">Back</a>
</h2>
<%=form_for  :provice, url: admin_provices_path do |f|%>
	<div class="row">
        <div class="input-field col s12">
          <%=f.text_field :name %>
          <label for="email">Provice Name</label>
        </div>
      </div>
      <button style="margin-right:20px;" class="btn waves-effect waves-light" type="submit" name="action">Create Provice</button>
<%end%>