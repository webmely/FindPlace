<h2 class="header">Type Management</h2>
<table class="striped hoverable">
	<tr>
		<th>ID</th>
		<th>Name</th>
		<th>Description</th>
		<th>Created_at</th>
		<th></th>
	</tr>
	<% @types.each do |t| %>
		<tr>
			<td><%=t.id %></td>
			<td><%=t.typename %></td>
			<td><%=t.description %></td>
			<td><%=t.created_at.strftime("%d - %m - %Y %H:%M:%S") %></td>
			<td></td>
		</tr>
	<% end %>
</table>