<h2 class="header">New Type 
	<a class="right waves-effect waves-light btn" href="/admin/types">Back</a>
</h2>
<%=form_for  :type, url: admin_types_path do |f|%>
	<div class="row">
        <div class="input-field col s12">
          <%=f.text_field :typename %>
          <label for="email">Type Name</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <%=f.text_field :description %>
          <label for="email">Description</label>
        </div>
      </div>
      <button style="margin-right:20px;" class="btn waves-effect waves-light" type="submit" name="action">Create Type</button>
<%end%>